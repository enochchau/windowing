{
  "name": "windowing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @windowing/demo dev",
    "build": "pnpm --filter @windowing/core build && pnpm --filter @windowing/demo build",
    "build:core": "pnpm --filter @windowing/core build",
    "build:demo": "pnpm --filter @windowing/demo build",
    "lint": "eslint packages/**/*.{ts,tsx}",
    "lint:fix": "eslint packages/**/*.{ts,tsx} --fix",
    "format": "prettier --write \"packages/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"packages/**/*.{ts,tsx,json,md}\"",
    "type-check": "pnpm --filter @windowing/core type-check && pnpm --filter @windowing/demo type-check",
    "check": "pnpm type-check && pnpm lint && pnpm format:check",
    "preview": "pnpm --filter @windowing/demo preview"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5"
  }
}
